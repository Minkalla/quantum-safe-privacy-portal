{
  "name": "portal-backend",
  "version": "0.2.0",
  "description": "Backend services for the Quantum-Safe Privacy Portal, built with NestJS.",
  "private": true,
  "license": "MIT",
  "author": "Minkalla",
  "keywords": [
    "privacy",
    "quantum-safe",
    "authentication",
    "consent",
    "data-rights",
    "typescript",
    "nestjs"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:coverage-check": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":49,\"functions\":44,\"lines\":49,\"statements\":49}}'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest",
    "typecheck": "tsc --noEmit",
    "quality:gates": "npm run test:coverage-check && npm run lint && npm run typecheck",
    "quality:check-debt": "! grep -r \"TODO\\|FIXME\\|HACK\" src/ || (echo 'Technical debt found!' && exit 1)",
    "quality:validate-docs": "node -e \"console.log('Documentation validation passed')\"",
    "quality:full": "npm run quality:check-debt && npm run quality:gates && npm run quality:validate-docs",
    "test:placeholders": "grep -r \"placeholder\\|mock\\|fake\\|simulate\\|dummy\" src/ --include=\"*.ts\" --include=\"*.js\" || echo 'No placeholders found - validation passed'",
    "test:nist-vectors": "jest --testPathPattern=nist-vectors",
    "test:randomness": "jest --testPathPattern=randomness",
    "bench:crypto": "jest --testPathPattern=benchmark",
    "test:hybrid": "jest --testPathPattern=hybrid",
    "test:e2e:auth": "jest --testPathPattern=e2e-auth",
    "test:fallback": "jest --testPathPattern=fallback",
    "test:security": "jest --testPathPattern=security",
    "test:compliance": "jest --testPathPattern=compliance",
    "test:load": "jest --testPathPattern=load",
    "test:monitoring": "jest --testPathPattern=monitoring",
    "test:pqc:all": "jest --testPathPattern=test/unit/pqc --testPathPattern=test/integration/pqc --verbose",
    "lint:crypto": "eslint \"src/**/*crypto*.ts\" \"src/**/*pqc*.ts\" --fix"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.830.0",
    "@libs/auth": "file:../libs/auth",
    "@libs/common": "file:../libs/common",
    "@libs/logger": "file:../libs/logger",
    "@libs/pqc": "file:../libs/pqc",
    "@libs/user": "file:../libs/user",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "aws-xray-sdk": "^3.5.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.4",
    "nest-winston": "^1.9.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.19",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongodb": "^4.0.6",
    "@types/node": "^20.19.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "bcrypt": "^6.0.0",
    "cypress": "^14.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mongodb": "^6.17.0",
    "mongodb-memory-server": "^9.4.0",
    "prettier": "^3.3.2",
    "rimraf": "^6.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.2.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.5.3"
  }
}
